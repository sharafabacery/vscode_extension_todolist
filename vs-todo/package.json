{
	"name": "vs-todo",
	"displayName": "vs-todo",
	"description": "to do list",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:vsto-sidebar",
		"onCommand:vs-todo.helloWorld",
		"onCommand:vs-todo.askQuestion",
		"onCommand:vs-todo.refresh",
		"onCommand:vs-todo.authenticate",
		"onCommand:vs-todo.addTodo"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "vsto-sidebar-view",
					"title": "vsto",
					"icon": "media/checklist.svg"
				}
			]
		},
		"views": {
			"vsto-sidebar-view": [
				{
					"type": "webview",
					"id": "vsto-sidebar",
					"name": "vsto",
					"icon": "media/checklist.svg",
					"contextualTitle": "vsto"
				}
			]
		},
		"commands": [
			{
				"command": "vs-todo.helloWorld",
				"category": "vs-todo",
				"title": "Hello World"
			},
			{
				"command": "vs-todo.authenticate",
				"category": "vs-todo",
				"title": "authenticate"
			},
			{
				"command": "vs-todo.askQuestion",
				"category": "vs-todo",
				"title": "askQuestion"
			},
			{
				"command": "vs-todo.refresh",
				"category": "vs-todo",
				"title": "refresh"
			},
			{
				"command": "vs-todo.addTodo",
				"category": "vs-todo",
				"title": "addTodo"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack --config ./build/node-extension.webpack.config.js",
		"watch": "concurrently \"rollup -c -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
		"package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^14.0.0",
		"@rollup/plugin-node-resolve": "^11.0.1",
		"@rollup/plugin-typescript": "^8.1.0",
		"@tsconfig/svelte": "^1.0.10",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/polka": "^0.5.1",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"concurrently": "^5.3.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"rollup": "^2.3.4",
		"rollup-plugin-svelte": "^6.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.31.1",
		"svelte-check": "^1.1.24",
		"svelte-preprocess": "^4.6.1",
		"ts-loader": "^8.0.11",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"polka": "^0.5.2"
	}
}
